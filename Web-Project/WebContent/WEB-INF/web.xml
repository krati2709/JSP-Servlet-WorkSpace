<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Web-Project</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    <error-page>
		<error-code>404</error-code>
		<location>/Erorr404.jsp</location>
	</error-page>

	<error-page>
		<error-code>500</error-code>
		<location>/Erorr500.jsp</location>
	</error-page>

	<servlet>
		<servlet-name>HelloCtl</servlet-name>
		<servlet-class>com.rays.servlet.HelloCtl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloCtl</servlet-name>
		<url-pattern>/HelloCtl</url-pattern>
	</servlet-mapping>

    
</web-app>